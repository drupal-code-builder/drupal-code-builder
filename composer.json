{
  "name": "drupal-code-builder/drupal-code-builder",
  "description": "Code generator for Drupal",
  "license": "GPL-2.0+",
  "require": {
    "php": "^7.4 || ^8",
    "joachim-n/mutable-typed-data": "^1.1.4",
    "joachim-n/case-converter": "^1.0",
    "php-di/php-di": "^6.2",
    "ckr/arraymerger": "^2.0",
    "nikic/php-parser": "^4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "drupal/coder": "^8.3",
    "squizlabs/php_codesniffer": "^3",
    "symfony/yaml": "^3.2",
    "symfony/var-dumper": "^3.2",
    "morrislaptop/var-dumper-with-context": "^0.1.0",
    "phpspec/prophecy-phpunit": "^2.0",
    "dms/phpunit-arraysubset-asserts": "^0.3.0"
  },
  "autoload": {
    "psr-4": {
      "DrupalCodeBuilder\\": ""
    }
  },
  "scripts": {
    "cr": "DrupalCodeBuilder\\DependencyInjection\\ContainerBuilder::rebuildCachedContainer"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
