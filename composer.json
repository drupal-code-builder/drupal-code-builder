{
  "name": "drupal-code-builder/drupal-code-builder",
  "description": "Code generator for Drupal",
  "license": "GPL-2.0+",
  "require": {
    "php": "^8",
    "joachim-n/mutable-typed-data": "^1.2",
    "joachim-n/case-converter": "^1.0",
    "php-di/php-di": "^7",
    "ckr/arraymerger": "^3.0",
    "nikic/php-parser": "^4.18 || ^5.0",
    "symfony/finder": "^4.0 || ^5 || ^6 || ^7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "drupal/coder": "^8.3",
    "mikey179/vfsstream": "^1.6.11",
    "squizlabs/php_codesniffer": "^3",
    "symfony/yaml": "^6",
    "symfony/var-dumper": "^6",
    "morrislaptop/var-dumper-with-context": "^0.1.0",
    "phpspec/prophecy-phpunit": "^2.0",
    "dms/phpunit-arraysubset-asserts": "^0.3.0"
  },
  "autoload": {
    "psr-4": {
      "DrupalCodeBuilder\\": ""
    }
  },
  "scripts": {
    "dcb-cr": "DrupalCodeBuilder\\DependencyInjection\\ContainerBuilder::rebuildCachedContainer"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
